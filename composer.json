{
    "name": "sailing-cz/api-client",
    "description": "Sailing.cz API client library",
    "type": "library",
    "license": "Apache-2.0",
    "autoload": {
        "psr-4": {
            "Sailing\\ApiClient\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Milan Sorm",
            "email": "sorm@sailing.cz"
        }
    ],
    "require": {
        "php": ">= 8.0",
        "ext-curl": "*",
        "ext-json": "*",
        "guzzlehttp/guzzle": ">= 7.5",
        "nette/utils": ">= 3",
        "ext-mbstring": "*",
        "ext-openssl": "*"
    },
    "prefer-stable": true,
    "require-dev": {
        "nette/tester": ">= 2.5"
    }
}
